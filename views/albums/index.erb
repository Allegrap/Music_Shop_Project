<div class="separate-albums">
  <% @albums.each do|album| %>

    <div class="separate-in-albums">
      <a href="/albums/<%= album.id() %>"><img class="artist-art" src=<%= album.image_url %>></a>

      <div class="artist">
        <h3><%= album.title %></h3>
        <p><%= album.artist.name %> </p>
      </div>
    </div>
    <br/>
  <% end %>
</div>

