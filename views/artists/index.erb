<div class="separate-albums">
  <% @artists.each do|artist| %>

    <div class="separate-in-albums">
      <a href="/artists/<%= artist.id %>"><img class="artist-art" src=<%= artist.image_url %>></a>

      <div class="artist">
        <h3><%= artist.name %></h3>
      </div>
    </div>

    <br/>
  <% end %>
</div>

